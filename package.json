{
  "name": "eslint-config-decent",
  "version": "2.8.24",
  "description": "A decent ESLint configuration",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "tsc --noEmit --skipLibCheck",
    "lint:markdown": "prettier --cache --write '*.md' '!**/(node_modules|dist)/**/*.md' && markdownlint '*.md' '!**/(node_modules|dist)/**/*.md' --config=.github/linters/.markdown-lint.yml --fix",
    "lint:code": "eslint --fix",
    "prelint": "npm run build",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "beta": "npm publish --tag beta",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.md": [
      "prettier --write --cache",
      "markdownlint --config=.github/linters/.markdown-lint.yml --fix"
    ],
    "*.{json5,yml}": [
      "prettier --write"
    ],
    "*.{js,cjs,mjs,ts}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgeurts/eslint-config-decent.git"
  },
  "keywords": [
    "eslint",
    "config",
    "decent"
  ],
  "author": "Jim Geurts <jim@biacreations.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgeurts/eslint-config-decent/issues"
  },
  "homepage": "https://github.com/jgeurts/eslint-config-decent#readme",
  "engines": {
    "node": ">=20.11.0"
  },
  "dependencies": {
    "@eslint/compat": "1.3.1",
    "@eslint/js": "9.29.0",
    "@next/eslint-plugin-next": "15.3.4",
    "@stylistic/eslint-plugin": "5.0.0",
    "@typescript-eslint/utils": "8.35.0",
    "@vitest/eslint-plugin": "1.3.3",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-jsdoc": "51.2.3",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-mocha": "11.1.0",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "6.0.0",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-testing-library": "7.5.3",
    "eslint-plugin-unicorn": "59.0.1",
    "globals": "16.2.0",
    "typescript-eslint": "8.35.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.3",
    "@semantic-release/npm": "12.0.2",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@swc/core": "1.12.7",
    "@types/node": ">=22",
    "eslint": "9.29.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "markdownlint-cli": "0.45.0",
    "npm-run-all2": "8.0.4",
    "pinst": "3.0.0",
    "prettier": "3.6.2",
    "semantic-release": "24.2.5",
    "typescript": "5.8.3",
    "unbuild": "3.5.0"
  },
  "overrides": {
    "eslint-plugin-jsx-a11y": {
      "eslint": "$eslint"
    },
    "eslint-plugin-react": {
      "eslint": "$eslint"
    },
    "eslint-plugin-react-hooks": {
      "eslint": "$eslint"
    },
    "eslint-plugin-testing-library": {
      "eslint": "$eslint"
    }
  }
}
